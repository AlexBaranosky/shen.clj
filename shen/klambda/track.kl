(defun shen-terpri-or-read-char ()
 (if (value shen-*step*) (shen-check-byte (read-byte (value *stinput*)))
  (nl 1)))

(defun shen-check-byte (V1140)
 (cond ((= V1140 (shen-hat)) (interror "aborted" ())) (true true)))

(defun shen-input-track (V1141 V1142 V1143)
