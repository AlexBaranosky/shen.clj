(defun shen-f_error (V1119)
 (do (intoutput "partial function ~A;~%" (@p V1119 ()))
  (do
   (if
    (and (not (shen-tracked? V1119))
     (y-or-n? (intmake-string "track ~A? " (@p V1119 ()))))
    (shen-track-function (ps V1119)) shen-ok)
   (simple-error "aborted"))))

(defun shen-tracked? (V1120) (element? V1120 (value shen-*tracking*)))

(defun track (V1121) (let Source (ps V1121) (shen-track-function Source)))

(defun shen-track-function (V1122)
 (cond
  ((and (cons? V1122)
    (and (= defun (hd V1122))
     (and (cons? (tl V1122))
      (and (cons? (tl (tl V1122)))
       (and (cons? (tl (tl (tl V1122))))
        (= () (tl (tl (tl (tl V1122))))))))))
   (let KL
    (cons defun
     (cons (hd (tl V1122))
      (cons (hd (tl (tl V1122)))
       (cons
        (shen-insert-tracking-code (hd (tl V1122)) (hd (tl (tl V1122)))
         (hd (tl (tl (tl V1122)))))
        ()))))
    (let Ob (eval KL)
     (let Tr (set shen-*tracking* (cons Ob (value shen-*tracking*))) Ob))))
  (true (shen-sys-error shen-track-function))))

