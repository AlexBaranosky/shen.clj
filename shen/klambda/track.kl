  (intoutput "~%~A<~A> Inputs to ~A ~%~A"
   (@p (shen-spaces V1141)
    (@p V1141 (@p V1142 (@p (shen-spaces V1141) (@p V1143 ()))))))
  (shen-recursively-print V1143)))

(defun shen-recursively-print (V1144)
 (cond ((= () V1144) (intoutput " ==>" ()))
  ((cons? V1144)
   (do (print (hd V1144))
    (do (intoutput ", " ()) (shen-recursively-print (tl V1144)))))
  (true (shen-sys-error shen-recursively-print))))

(defun shen-spaces (V1145)
 (cond ((= 0 V1145) "") (true (cn " " (shen-spaces (- V1145 1))))))

(defun shen-output-track (V1146 V1147 V1148)
