  ((= V1095 (vector 0)) "<>")
  ((cons? V1095)
   (shen-cn-all
    (append (if (= V1094 "R") (cons "(" ()) (cons "[" ()))
     (append (cons (shen-ob->str V1094 (hd V1095)) ())
      (append
       (shen-xmapcan (value *maximum-print-sequence-size*)
        (lambda Z (cons " " (cons (shen-ob->str V1094 Z) ()))) (tl V1095))
       (if (= V1094 "R") (cons ")" ()) (cons "]" ())))))))
  ((vector? V1095)
   (let L (shen-vector->list V1095 1)
    (let E
     (tlstr
      (shen-cn-all
       (shen-xmapcan (- (value *maximum-print-sequence-size*) 1)
        (lambda Z
         (cons " " (cons (shen-ob->str V1094 (shen-blank-fail Z)) ())))
        L)))
     (let V (cn "<" (cn E ">")) V))))
  ((and (not (string? V1095)) (absvector? V1095))
   (trap-error (shen-ob->str "A" ((<-address V1095 0) V1095))
    (lambda Ignore
     (let L (shen-vector->list V1095 0)
      (let E
       (tlstr
        (shen-cn-all
         (shen-xmapcan (- (value *maximum-print-sequence-size*) 1)
          (lambda Z (cons " " (cons (shen-ob->str V1094 Z) ()))) L)))
       (let V (cn "<" (cn E ">")) V))))))
  ((= shen-vector-failure-object V1095) "...")
  (true (if (and (= V1094 "A") (string? V1095)) V1095 (str V1095)))))

