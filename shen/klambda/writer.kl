(defun shen-ms-h (V1090 V1091)
 (cond ((= () V1090) "")
  ((and (cons? V1090)
    (and (= "~" (hd V1090))
     (and (cons? (tl V1090)) (= "%" (hd (tl V1090))))))
   (cn (n->string 10) (shen-ms-h (tl (tl V1090)) V1091)))
  ((and (cons? V1090)
    (and (= "~" (hd V1090))
     (and (cons? (tl V1090))
      (and (tuple? V1091)
       (element? (hd (tl V1090)) (cons "A" (cons "S" (cons "R" ()))))))))
   (cn (shen-ob->str (hd (tl V1090)) (fst V1091))
    (shen-ms-h (tl (tl V1090)) (snd V1091))))
  ((cons? V1090) (cn (hd V1090) (shen-ms-h (tl V1090) V1091)))
  (true (shen-sys-error shen-ms-h))))

(defun shen-ob->str (V1094 V1095)
